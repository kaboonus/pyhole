table#logTable:
	font-size: 13px
	width: 100%

	tr:
		th,td:
			padding: 2px 4px
			border-style: solid
			border-width: 1px 0 0 1px
			border-color: #333
			&:last-child:
				border-width: 1px
		&:last-child td:
			border-width: 1px
		&:nth-child(even):
			background-color: #222

			th,td.time:
				width: 180px

			th,td.user:
				min-width: 80px

			th,td.message:
				min-width: 100px

